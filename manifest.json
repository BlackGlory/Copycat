{
  "version": "1.0.0",
  "name": "Copycat",
  "manifest_version": 2,
  "description": "Copy content from web powerful than ever before.",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["inject.js"]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
